{
  "expo": {
    "name": "bitwise-inventory",
    "slug": "bitwise-inventory",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "extra": {
      "API_URL": "https://inventory.bitwisepctrading.com/api",
      "SECRET_KEY": "ec433db5199c2cfdd5565df3121cdbe737a1e425bbe10e6d651aba4c4e1a3894"
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
